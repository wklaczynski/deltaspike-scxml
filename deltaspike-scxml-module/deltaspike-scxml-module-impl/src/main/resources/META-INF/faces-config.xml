<?xml version='1.0' encoding='UTF-8'?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:bridge="http://www.portletfaces.org/xml/ns/portletfaces-bridge-2.0-extension"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
    http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
    version="2.0">

    <application>
        <navigation-handler>org.apache.deltaspike.scxml.impl.faces.DialogNavigationHandler</navigation-handler>
        <el-resolver>org.apache.deltaspike.scxml.impl.invokers.ViewELResolver</el-resolver>
        <el-resolver>org.apache.deltaspike.scxml.impl.DialogScopeELResolver</el-resolver>
        <state-manager></state-manager>
    </application>

    <factory>
        <view-declaration-language-factory>org.apache.deltaspike.scxml.impl.faces.DialogViewDeclarationLanguageFactory</view-declaration-language-factory>
    </factory>


    
    
</faces-config>